# Environment Configuration
NODE_ENV=development
PORT=3199

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/voice_diagnosis
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=3600

# TEN Framework Configuration
TEN_FRAMEWORK_URL=https://api.theten.ai
TEN_API_KEY=your-ten-api-key
TEN_AGENT_ID=your-agent-id
TEN_GRAPH_ID=voice-assistant

# SiliconFlow API Configuration (OpenAI-compatible alternative)
SILICONFLOW_API_KEY=your-siliconflow-api-key
SILICONFLOW_BASE_URL=https://api.siliconflow.com/v1

# Optional: Keep other services if needed, or disable them
# DEEPGRAM_API_KEY=your-deepgram-api-key
# ELEVENLABS_API_KEY=your-elevenlabs-api-key
# AGORA_APP_ID=your-agora-app-id
# AGORA_APP_CERTIFICATE=your-agora-app-certificate

# Legacy OpenAI support (can be used as fallback)
# OPENAI_API_KEY=your-openai-api-key

# TEN Framework Multimodal Configuration
TEN_MULTIMODAL_ENABLED=true
TEN_AUDIO_EXTENSION_ENABLED=true
TEN_IMAGE_EXTENSION_ENABLED=true
TEN_VIDEO_EXTENSION_ENABLED=true
TEN_SENSOR_EXTENSION_ENABLED=true

# Medical AI Configuration
MEDICAL_MODEL_PATH=./models/medical_models/
AUDIO_PROCESSING_ENABLED=true
IMAGE_PROCESSING_ENABLED=true
CLINICAL_DATA_ENABLED=true
VIDEO_PROCESSING_ENABLED=true
SENSOR_DATA_ENABLED=true

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key
CORS_ORIGIN=http://localhost:3199

# File Upload Configuration
MAX_FILE_SIZE=50MB
UPLOAD_PATH=./uploads/
TEMP_PATH=./temp/

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Monitoring Configuration
PROMETHEUS_PORT=9090
HEALTH_CHECK_INTERVAL=30000

# Medical Compliance
HIPAA_COMPLIANCE=true
AUDIT_LOGGING=true
DATA_RETENTION_DAYS=2555

# Performance Configuration
MAX_CONCURRENT_ANALYSES=10
ANALYSIS_TIMEOUT=300000
CACHE_TTL=3600
